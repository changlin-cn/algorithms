{
  "name": "@hechanglin/algorithms",
  "version": "1.2.4",
  "description": "algorithms",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "jest",
    "build:js": "rollup -c",
    "build": "npm run build:js && npm run changelog && npm run doc",
    "release": "npm run build && standard-version",
    "precommit": "npm run test && lint-staged",
    "op-time-sort": "ts-node  --project tsconfig.server.json operation-time/sort.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "doc": "typedoc --plugin typedoc-plugin-markdown src/index.ts --readme none"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.ts?(x)": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/changlin-cn/algorithms.git"
  },
  "keywords": [
    "algorithms"
  ],
  "author": "Hechanglin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/changlin-cn/algorithms/issues"
  },
  "homepage": "https://github.com/changlin-cn/algorithms#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@types/jest": "^26.0.19",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.16.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "rollup": "^2.35.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "standard-version": "^9.1.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.19",
    "typedoc-plugin-markdown": "^3.4.5",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5"
  },
  "standard-version": {
    "skip": {
      "changelog": true
    }
  }
}
